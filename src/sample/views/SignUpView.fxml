<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.SignUpController">
    <children>
        <TextField fx:id="nameField" layoutX="34.0" layoutY="46.0" prefHeight="44.0" prefWidth="187.0" promptText="Имя">
            <font>
                <Font size="24.0" />
            </font></TextField>
        <TextField fx:id="surnameField" layoutX="278.0" layoutY="46.0" prefHeight="44.0" prefWidth="187.0" promptText="Фамилия">
            <font>
                <Font size="24.0" />
            </font></TextField>
        <TextField fx:id="patronymicField" layoutX="522.0" layoutY="46.0" prefHeight="44.0" prefWidth="187.0" promptText="Отчество">
            <font>
                <Font size="24.0" />
            </font></TextField>
        <TextField fx:id="cityField" layoutX="34.0" layoutY="117.0" prefHeight="44.0" prefWidth="284.0" promptText="Город">
            <font>
                <Font size="24.0" />
            </font></TextField>
        <TextField fx:id="dateField" layoutX="34.0" layoutY="190.0" prefHeight="44.0" prefWidth="284.0" promptText="ГГГГ-ММ-ДД">
            <font>
                <Font size="24.0" />
            </font></TextField>
        <TextField fx:id="numberField" layoutX="34.0" layoutY="262.0" prefHeight="44.0" prefWidth="284.0" promptText="70000000000">
            <font>
                <Font size="24.0" />
            </font>
        </TextField>
        <TextField fx:id="loginField" layoutX="34.0" layoutY="331.0" prefHeight="44.0" prefWidth="284.0" promptText="Логин">
            <font>
                <Font size="24.0" />
            </font>
        </TextField>
        <TextField fx:id="passwordField" layoutX="34.0" layoutY="403.0" prefHeight="44.0" prefWidth="284.0" promptText="Пароль">
            <font>
                <Font size="24.0" />
            </font>
        </TextField>
        <Button fx:id="helpPhoneBtn" layoutX="332.0" layoutY="190.0" mnemonicParsing="false" onAction="#helpPhoneBtnAction" prefHeight="51.0" prefWidth="51.0" text="?">
            <font>
                <Font size="24.0" />
            </font>
        </Button>
        <Button fx:id="cancelBtn" layoutX="871.0" layoutY="44.0" mnemonicParsing="false" onAction="#cancelBtnAction" prefHeight="40.0" prefWidth="65.0" text="&lt;">
            <font>
                <Font size="24.0" />
            </font>
        </Button>
        <Button fx:id="signUpBtn" layoutX="706.0" layoutY="485.0" mnemonicParsing="false" onAction="#signUpBtnAction" prefHeight="63.0" prefWidth="264.0" text="Зарегистрироваться">
            <font>
                <Font size="24.0" />
            </font>
        </Button>
      <TextField fx:id="citizenField" layoutX="422.0" layoutY="117.0" prefHeight="44.0" prefWidth="284.0" promptText="Гражданство">
         <font>
            <Font size="24.0" />
         </font>
      </TextField>
      <TextField fx:id="pointField" layoutX="422.0" layoutY="194.0" prefHeight="44.0" prefWidth="284.0" promptText="Цель кредита">
         <font>
            <Font size="24.0" />
         </font>
      </TextField>
      <Button layoutX="727.0" layoutY="114.0" mnemonicParsing="false" onAction="#helpCitizenBtnAction" prefHeight="51.0" prefWidth="51.0" text="?">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <TextField fx:id="salaryField" layoutX="422.0" layoutY="262.0" prefHeight="44.0" prefWidth="284.0" promptText="Ваша зарплата">
         <font>
            <Font size="24.0" />
         </font>
      </TextField>
    </children>

</AnchorPane>
